{
    "name": "noistudio/artcrud",
    "description": "Laravel library for automatic generation CRUD Controllers",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Artem Tatarinov",
            "email": "artem@noibiz.com"
        }

    ],
    "require": {

        "laravel/framework": "^8.12|^9.0"
    },
    "require-dev": {
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Artcrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artcrud\\Frameworks\\Illuminate\\ServiceProvider"
            ]

        }
    },
    "minimum-stability": "dev"
}
